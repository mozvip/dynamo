<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">

	<listener>
		<listener-class>dynamo.ui.DynamoContextListener</listener-class>
	</listener>

	<servlet>
		<servlet-name>SuggestMusicArtist</servlet-name>
		<servlet-class>dynamo.ui.servlets.SuggestMusicArtist</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SuggestMusicArtist</servlet-name>
		<url-pattern>/suggest-music-artist/*</url-pattern>
	</servlet-mapping>

</web-app>